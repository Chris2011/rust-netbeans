<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Editors">
        <folder name="text">
            <folder name="x-rust-source">
                <attr name="displayName" bundlevalue="com.github.drrb.rust.netbeans.Bundle#Editors/text/x-rust-source"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="FontAndColors.xml"><!-- Don't "Use displayName instead": this is where we keep the token type names -->
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.github.drrb.rust.netbeans.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>

    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-rust-source" url="RustPreviewExample.rs"/>
            </folder>
        </folder>
    </folder>

</filesystem>
